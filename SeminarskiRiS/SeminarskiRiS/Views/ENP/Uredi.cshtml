@using SeminarskiRS1.Models
@model SeminarskiRS1.ViewModels.ENP.ENPUrediVM
@{
    ViewData["Title"] = "Uredi";
}

<h1>Uredi</h1>

<form action="/ENP/Snimi" method="post">
    @Html.AntiForgeryToken()
    @Html.HiddenFor(s => s.ENPID)
    <label>Naziv</label>
    @Html.TextBoxFor(s => s.Naziv)
    <br />
    <label>Broj Telefona</label>
    @Html.TextBoxFor(s => s.BrTelefona)
    <br />
    <label>Email</label>
    @Html.TextBoxFor(s => s.Email)
    <br />
    <label>Radno Vrijeme</label>
    @Html.TextBoxFor(s => s.RadnoVrijeme)
    <br />
    <label>Grad</label>
    @Html.DropDownListFor(s => s.GradID,Model.gradovi)

    <input type="submit" value="Snimi" />
</form>

